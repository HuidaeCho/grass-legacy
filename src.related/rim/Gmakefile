
# make rim (generic instructions)
#
all: rim librim.a

rim::
	sh run.setup
	rm -f src/rimlib
	find src -name "*.o" -exec rm -f {} \;
	make -f Makefile 'rim'
	mv 'rim' $(UNIX_BIN)/rim
	make -f Makefile 'librim.a'
	$(GMAKE) -makeparentdir librim.a
	mv 'librim.a' librim.a
	find src -name "*.o" -exec rm -f {} \;


