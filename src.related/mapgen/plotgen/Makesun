# "@(#)Makesun	OEMG v.1.3";

CFLAGS = -O -I$(INCL) -I../mapgen -I./

LCORE = -lcore -lsunwindow -lpixrect

LDFLAGS = -s

LLIB = $(MSRC)/libplot.a $(MSRC)/lib/libgen.a

LIST = szoom.o s_core.o

szoom:	$(LIST)
	cc $(LDFLAGS) -o szoom $(LIST) $(LLIB) $(LCORE) -lm

install: szoom
	mv szoom $(BIN)/szoom

