###########################################################
#  %W%  %G%
UNIX_BIN = $(GRAPH)/macros

LIBDIG		= $(SRC)/mapdev/lib/libdig.a

EXTRA_CFLAGS	= -I$(SRC)/mapdev/lib   -I$(SRC)/mapdev/libes


OFILES = \
	dig_to_geo.o



all:  $(UNIX_BIN)/dig.to.geo 

$(UNIX_BIN)/dig_to_geo: $(OFILES)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBDIG) $(GISLIB)

$(UNIX_BIN)/dig.to.geo: dig.to.geo $(UNIX_BIN)/dig_to_geo
	cp dig.to.geo $(UNIX_BIN)/dig.to.geo

