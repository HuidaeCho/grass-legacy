
###########################################################


###########################################################
#  %W%  %G%


UNIX_BIN = $(GRAPH)/macros


CFILE = make_box.c

all:  $(UNIX_BIN)/make_box $(UNIX_BIN)/make.box $(UNIX_BIN)/neatline 

$(UNIX_BIN)/make_box: $(CFILE)
	$(CC) -o $@ $(CFILE)

$(UNIX_BIN)/make.box: make.box
	cp make.box $(UNIX_BIN)/make.box

$(UNIX_BIN)/neatline: neatline
	cp neatline $(UNIX_BIN)/neatline

