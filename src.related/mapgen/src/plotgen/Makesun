# "@(#)Makesun	OEMG v.1.1";

INCL = $(GRAPH)/src/include

BIN = $(GRAPH)/bin

CFLAGS = -O -I$(INCL) -DGRAPHB=\"GRAPHB\"

LCORE = -lcore -lsunwindow -lpixrect

LDFLAGS = -s

LLIB = libplot.a $(GRAPH)/src/lib/libgen.a

LIST = szoom.o s_core.o

szoom:	$(LIST)
	cc $(LDFLAGS) -o szoom $(LIST) $(LLIB) $(LCORE) -lm

install:
	make -f Makesun szoom
	mv szoom $(BIN)/szoom
	rm -f szoom.o s_core.o

