#include "glob.h"
#include "G3d.h"
from_3dcellhd()
{
    char name[30];
    char *mapset;
    G3D_Region window;
    char fullName[1000];

    mapset = G_ask_old ("", name, G3D_DIRECTORY, "3d raster");
    if (!mapset) return 1;

    G3d_filename (fullName, G3D_HEADER_ELEMENT, name, mapset);
    if (G3d_readWindow (&window, fullName) == 0)
	fprintf (stderr, "header file for [%s in %s] invalid. can't select. ",
		name, mapset);
    else
    {
	if(!edit_window (&window)) return 1;
	set_window (&window, name);
    }
    return 0;
}
