OBJ=main.o binio.o v5d.o
SOURCES=main.c binio.c v5d.c

# For Linux please add this compile flag
EXTRA_CFLAGS = -DLITTLE

DIR=.

PGM=r3.in.v5d
HOME=$(BIN_MAIN_CMD)

$(HOME)/$(PGM): $(OBJ) $(G3DLIB) $(GISLIB) $(LINKMLIB)
	$(CC) $(LDFLAGS) -o $@ $(OBJ) $(G3DLIB) $(GISLIB) $(MATHLIB) $(LINKMLIB) $(XDRLIB)

$(GISLIB): #
