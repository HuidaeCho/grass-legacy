OBJ=main.o binio.o v5d.o
SOURCES=main.c binio.c v5d.c

# For Linux please add this compile flag
EXTRA_CFLAGS = -DLITTLE

PGM=r3.in.v5d

LIBES=$(G3DLIB) $(GISLIB) $(LINKMLIB)
DEPLIBS=$(DEPG3DLIB) $(DEPGISLIB) $(DEPLINKMLIB)

$(BIN_CMD)/$(PGM): $(OBJ) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OBJ) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPGISLIB): #
