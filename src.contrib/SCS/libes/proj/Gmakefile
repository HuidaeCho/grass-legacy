EXTRA_CFLAGS=$(CAN_CLEAR)
OBJ = SetProj.o
PROJSCS = $(LIBDIR)/scsplib.a

all: START $(PROJSCS)

$(PROJSCS): $(OBJ)
	$(AR)

SetProj.o: projects.h

START:
	cp $(SRC)/../src.related/mapgen/proj/SetProj.c SetProj.c
	cp $(SRC)/../src.related/mapgen/proj/projects.h projects.h

version.h: $(VERSION_FILE)
	echo 'static char *VERSION_STRING = "@(#) GRASS $(VERSION_NUMBER) ($(VERSION_DATE))";' > $@
...: #
