PGM      = v.dump


LIST =  main.o \
	do_areas.o \
	do_lines.o \
	isle_area.o \
	perimeter.o \
	head_info.o

all: $(BIN_CMD)/$(PGM)


$(BIN_CMD)/$(PGM): $(LIST) 
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(VECTLIB) $(VASKLIB) $(GISLIB) $(CURSES) $(MATHLIB) $(XDRLIB)

main.o: dump.h
do_areas.o: dump.h
do_lines.o: dump.h

$(DEPGISLIB): #
$(DEPVECTLIB): #
