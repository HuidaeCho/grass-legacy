LIBES = $(GISLIB) $(VECTLIB) $(COORCNVLIB) $(MATHLIB) $(EXTRALIB) $(DATETIMELIB)
DEPLIBS= $(DEPGISLIB) $(DEPVECTLIB) $(DEPCOORCNVLIB) $(MATHLIB) $(EXTRALIB) $(DEPDATETIMELIB)

OFILES = \
	mkquads_cm.o \
	ask_for.o \
	calc_quads.o \
	convert.o \
	init_head.o \
	init_quad.o \
	reg_quads.o \
	report_quads.o \
	sites_quads.o \
	wind_quads.o \
	write_quads.o

PROG = v.mkquads.scs

$(BIN_CMD)/$(PROG): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES)
