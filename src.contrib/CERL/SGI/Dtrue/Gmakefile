# %W%  %G%

DLIB  = $(SRC)/D/libes/Dlib.a

OFILES = main.o Dcell.o

Dtrue: $(BIN_CONTRIB_CMD)/Dtrue

$(BIN_CONTRIB_CMD)/Dtrue: $(OFILES) $(D_LIB) $(DISPLAYLIB) $(RASTERLIB) $(GISLIB)
	rm -f a.out $@
	$(CC) $(LDFLAGS) $(OFILES) $(D_LIB) $(DISPLAYLIB) $(RASTERLIB) $(GISLIB) -lgl_s
	mv a.out $@

$(RASTERLIB): #
$(DISPLAYLIB): #
$(GISLIB): #
$(D_LIB): #
