PGM = select_basins
RFEA = $(GISBASE)/etc/r.fea
LIBES = $(DISPLAYLIB) $(RASTERLIB) $(GISLIB)
LIST = main.o selectbasin.o basinlink.o unique.o 

$(RFEA)/$(PGM): $(LIST) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(XDRLIB) $(MATHLIB)

saber:
	$(CC) -o $@ *.c 
$LIBES: #

