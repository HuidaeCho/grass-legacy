$(HOME)/$(PGM): $(LIST) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(ROWIOLIB): # in case LIB changes
$(COORCNVLIB): # in case LIB changes
$(GISLIB): # in case LIB changes
