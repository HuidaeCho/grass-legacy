PGM=r.in.utm
HOME=$(BIN_CMD)

LIST = main.o isnumber.o sph.o row_col.o utm_value.o value.o

LIBES = $(COORCNVLIB) $(GISLIB) $(DATETIMELIB) $(ROWIOLIB)
DEPLIBS= $(DEPCOORCNVLIB) $(DEPGISLIB) $(DEPDATETIMELIB) $(DEPROWIOLIB)

$(HOME)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPROWIOLIB): # in case LIB changes
$(DEPCOORCNVLIB): # in case LIB changes
$(DEPGISLIB): # in case LIB changes
