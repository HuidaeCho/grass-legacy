PGM = sites_in_cell
I = ../Include
EXTRA_CFLAGS  = -I$I
SITELIB = ../lib.a
SITE = $(ETC)/sites

LIST =  main.o\
	new_screen.o\
        utm_to_rc.o

$(SITE)/$(PGM): $(LIST) $(SITELIB) $(DEPGISLIB)
		$(CC) ${LDFLAGS} -o $(SITE)/$(PGM) $(LIST) $(SITELIB) $(GISLIB) $(TERMLIB) $(MATHLIB) $(XDRLIB)

$(LIST):	$I/site.h  site_count.h
$(SITELIB):	#
$(DEPGISLIB):	#
