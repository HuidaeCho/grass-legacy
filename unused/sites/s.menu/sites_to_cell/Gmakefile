PGM = sites_to_cell
I = ../Include
EXTRA_CFLAGS  = -I$I
SITELIB = ../lib.a
SITE = $(ETC)/sites

LIST =  main.o\
	ask_quad.o\
	new_screen.o\
	write_cell.o

$(SITE)/$(PGM): $(LIST) $(SITELIB) $(DEPGISLIB)
		$(CC) ${LDFLAGS} -o $(SITE)/$(PGM) $(LIST) $(SITELIB) $(GISLIB) $(TERMLIB) $(MATHLIB) $(XDRLIB)

$(LIST):	$I/site.h
$(SITELIB):	#
$(DEPGISLIB):	#
