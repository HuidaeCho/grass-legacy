PROG = ../bin/tcltkgrass
INC = -I ./../include

EXTRA_CFLAGS = $(TCLINCDIR) $(TKINCDIR) $(INC)

TCLTKLIB = $(TCLTKLIBS) -ldl

LIBS  = $(VECTLIB) $(GISLIB) $(DATETIMELIB)
LLIBS = $(TCLTKLIB) $(XLIBPATH) $(XLIB) $(MATHLIB)

OBJ=\
	georeg.o\
	layer.o\
	view.o\
	draw.o\
	main.o\
	do_labels.o

all: $(PROG) 
# --fwritable-strings
$(PROG): $(OBJ) $(LIBS)
	${CC} $(LDFLAGS) -o $@ $(OBJ) $(GISLIB) $(LIBS) $(LLIBS)

$(LIBS): #
