PGM       = $(BIN_CMD)/v.reclass.pg
# EXTRA_CFLAGS = -Dunlink=puts
LIBES     = $(DISPLAYLIB) $(GISLIB) 
DEPLIBS= $(DEPDISPLAYLIB) $(DEPGISLIB) 
PACKAGE = "v.reclass.pg"
DEFS = -Wall -DPACKAGE=\"$(PACKAGE)\"
EXTRA_CFLAGS    = $(PQINCPATH) $(DEFS)


OBJS = \
	main.o \
        getAllOpts.o \
        getSopts.o \
	buildPg.o \
	runPg.o

$(PGM): $(OBJS) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OBJS) $(LIBES) $(PQLIBPATH) $(PQLIB) $(MATHLIB) $(XDRLIB) $(INTLLIB)

$(DEPLIBS): #
