PGM       = $(BIN_CMD)/g.column.pg
LIBES     = $(GISLIB) 
DEPLIBS= $(DEPGISLIB) 
PACKAGE = "g.column.pg"
DEFS = -Wall -DPACKAGE=\"$(PACKAGE)\"
EXTRA_CFLAGS    = $(PQINCPATH) $(DEFS)

OBJS = \
	main.o \
	infxColumn.o


$(PGM): $(OBJS) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OBJS) $(LIBES) $(PQLIBPATH) $(PQLIB) $(MATHLIB) $(XDRLIB) $(INTLLIB)

$(DEPLIBS): #
