PGM = pg.in.dbf
HOME = $(BIN_CMD)
EXTRA_CFLAGS = $(PQINCPATH)
LIST = 	main.o \
	dbfopen.o \
	pgdump.o

$(HOME)/$(PGM): $(LIST) $(GISLIB) $(DATETIMELIB) $(XEXTRALIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(GISLIB) $(VECTLIB) $(VASKLIB) $(DATETIMELIB) $(XEXTRALIBS) $(CURSES) $(MATHLIB) $(XDRLIB) $(PQLIB)

$(GISLIB): # 
