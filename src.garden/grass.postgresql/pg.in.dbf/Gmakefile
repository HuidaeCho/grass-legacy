PGM = pg.in.dbf
HOME = $(BIN_CMD)
PACKAGE = "pg.in.dbf"
DEFS = -Wall -DPACKAGE=\"$(PACKAGE)\"
EXTRA_CFLAGS    = $(PQINCPATH) $(DEFS) -I/usr/include/pgsql -L/usr/lib -lpq

LIST = 	main.o \
	dbfopen.o \
	pgdump.o

$(HOME)/$(PGM): $(LIST) $(DEPGISLIB) $(DEPDATETIMELIB)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(GISLIB) $(VECTLIB) \
		$(DATETIMELIB) $(MATHLIB) $(XDRLIB) \
		$(PQLIBPATH) $(PQLIB) $(INTLLIB) -L/usr/lib -lpq

$(DEPGISLIB): # 
