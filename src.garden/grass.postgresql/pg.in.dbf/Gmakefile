PGM = pg.in.dbf
HOME = $(BIN_CMD)
EXTRA_CFLAGS = $(PQINCPATH)
LIST = 	main.o \
	dbfopen.o \
	pgdump.o

$(HOME)/$(PGM): $(LIST) $(GISLIB) $(DATETIMELIB)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(GISLIB) $(VECTLIB) \
		$(DATETIMELIB) $(XEXTRALIBS) $(MATHLIB) $(XDRLIB) \
		$(PQLIBPATH) $(PQLIB)

$(GISLIB): # 
