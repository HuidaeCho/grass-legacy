PGM       = $(BIN_CMD)/d.what.r.pg
LIBES     = $(DISPLAYLIB) $(RASTERLIB) $(GISLIB) 
PACKAGE = "d.what.r.pg"
DEFS = -Wall -DPACKAGE=\"$(PACKAGE)\"
EXTRA_CFLAGS    = $(PQINCPATH) $(DEFS)



OBJS = \
	main.o  \
        getAllOpts.o \
        getSopts.o \
        buildInfxQry.o \
        runInfxQry.o \
	getCat.o  \
	opencell.o 


$(PGM): $(OBJS) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(OBJS) $(LIBES) $(PQLIBPATH) $(PQLIB) $(XEXTRALIBS) $(MATHLIB) $(XDRLIB)

$(LIBES): #
