PGM       = $(BIN_CMD)/g.table.pg
EXTRA_CFLAGS = $(PQINCPATH) $(DEFS)
LIBES     = $(GISLIB) 
PACKAGE=g.table.pg

DEFS = -Wall -DLOCALEDIR=\"$(LOCALEDIR)\" -DPACKAGE=\"$(PACKAGE)\"

OBJS = \
	main.o \
	infxTable.o

$(PGM): $(OBJS) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(OBJS) $(LIBES) $(PQLIBPATH) $(PQLIB) $(XEXTRALIBS) $(MATHLIB) $(XDRLIB)

$(LIBES): #

