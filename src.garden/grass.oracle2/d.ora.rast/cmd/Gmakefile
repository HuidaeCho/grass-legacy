CAT1=`cat /u/app/oracle/product/8.0.5/lib/naldflgs`

AL1=-lnetv2 -lnttcp -lnetwork -lncr
AL2=-lnetv2 -lnttcp -lnetwork -lclient -lvsn -lcommon -lgeneric -lmm -lnlsrtl3
