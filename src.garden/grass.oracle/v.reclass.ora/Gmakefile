PGM       = $(BIN_CMD)/v.reclass.ora
LIBES     = $(DISPLAYLIB) $(VECTLIB) $(DIG2LIB) $(DIG_ATTLIB) $(RASTERLIB) $(GISLIB) 
DEPLIBS= $(DEPDISPLAYLIB) $(DEPVECTLIB) $(DEPDIG2LIB) $(DEPDIG_ATTLIB) $(DEPRASTERLIB) $(DEPGISLIB) 



OBJS = \
	main.o  \
	bsearch.o \
	find.o \
	infxQry.o \
	modCat.o 



$(PGM): $(OBJS) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OBJS) $(LIBES) $(XDRLIB) $(MATHLIB)

$(DEPLIBS): #
