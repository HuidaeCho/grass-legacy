    {entry south {Южная граница региона:} 0 ""}
    {entry east {Восточная граница региона:} 0 ""}
    {entry west {Западная граница региона:} 0 ""}
    {entry r {Число строк:} 0 ""}
    {entry c {Число столбцов:} 0 ""}
    {entry subst { Заменить первую величину на вторую (умол.: 0,0):} 0 ""}
