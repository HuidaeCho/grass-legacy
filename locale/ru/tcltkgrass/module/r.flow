interface_build {
    {r.flow} 0
    {Вычисляет кривизну стока (линии тока), длины путей и плотности линий тока по DEM.}
    {entry elevin {Растровая карта (DEM):} 0 raster}
    {entry aspin {Растровая карта экспозиций (выбор):} 0 raster}
    {entry barin {Растровая бинарная карта барьеров (выбор):} 0 raster}
    {entry skip {Задать число клеток между линий тока на рез. карте (выбор, умол.=max):} 0 ""}
    {entry bound {Задать макс. число сегментов линии тока (выбор):} 0 ""}
    {entry flout {Карта линий тока (рез.):} 0 vector}
    {entry lgout {Карта длин путей (выбор):} 0 raster}
    {entry dsout {Карта плотностей путей (выбор):} 0 raster}
    {checkbox -u {Вычислять линии вверх по склону(умол: вниз).} "" -u}
    {checkbox -3 {Вычислять трехмерные длины (умол: двумерные).} "" -3}
    {checkbox -m {Лим. память (без картты эксп., медленнее).} "" -m}
    {checkbox -M {Фикс. память (для больших файлов, намн. медленне).} "" -M}
    {checkbox -q {без комм.} "" -}

}
