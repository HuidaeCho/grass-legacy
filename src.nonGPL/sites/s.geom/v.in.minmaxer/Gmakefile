EXTRA_CFLAGS   =  -I$(SRC)/include/geom

LIBES = $(VECTLIB) $(DIG_ATTLIB) $(GISLIB) $(GEOMLIB) $(XDRLIB)
DEPLIBS= $(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPGISLIB) $(DEPGEOMLIB)

SOURCES = \
	main.c

OBJ = \
	main.o



PROG =   $(BIN_CMD)/v.in.minmaxer

all:  $(PROG)

$(PROG): $(OBJ) $(DEPLIBS)
	$(CC) $(EXTRA_FLAGS) $(LDFLAGS) -o $@ $(OBJ) $(LIBES) $(MATHLIB)

$(DEPLIBS): #
