EXTRA_CFLAGS   = -I$(SRC)/include/geom

LIBES = $(VECTLIB) $(DIG_ATTLIB) $(GISLIB) $(GEOMLIB) $(XDRLIB) $(DATETIMELIB)
DEPLIBS= $(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPGISLIB) $(DEPGEOMLIB) $(DEPDATETIMELIB)

SOURCES = voroSupport.c \
	  main.c

OBJ =   voroSupport.o \
	main.o



PROG =   $(BIN_CMD)/s.geom 

all:  $(PROG)

$(PROG): $(OBJ) $(DEPLIBS)
	$(CC) $(EXTRA_FLAGS) $(LDFLAGS) -o $@ $(OBJ) $(LIBES) $(MATHLIB)

$(DEPLIBS): #
