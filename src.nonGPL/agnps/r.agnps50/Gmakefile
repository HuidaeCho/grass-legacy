# $Id: Gmakefile,v 1.3 2000-09-19 14:24:07 markus Exp $

all:
	if [ ! -d $(GISBASE)/etc/agnps50 ]; then mkdir $(GISBASE)/etc/agnps50 ; fi
	$(GMAKE) agnps-source
	$(GMAKE) agview
	$(GMAKE) hydro_tools
	$(GMAKE) src_agnps_input_1
	$(GMAKE) src_agnps_input_2a
	cp scripts/r.agnps50.input $(GISBASE)/scripts/
	(cd scripts/ ; ls -1 | grep -v CVS | grep -v Gmakefile | tar -cvf - - | ( cd $(GISBASE)/etc/agnps50 ; tar -xvf - ) 2>/dev/null ; true )
