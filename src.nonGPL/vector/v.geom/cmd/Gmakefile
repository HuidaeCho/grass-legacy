# $Id: Gmakefile,v 1.4 2002-04-17 13:45:38 glynn Exp $

EXTRA_CFLAGS	=  -I$(SRC)/include/geom

LIBES = $(VECTLIB) $(DIG_ATTLIB) $(GISLIB) $(GEOMLIB)
DEPLIBS= $(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPGISLIB) $(DEPGEOMLIB)

SOURCES = \
	main.c

OBJ = \
	main.o

PROG =   $(BIN_CMD)/v.geom

all:  $(PROG)

$(PROG): $(OBJ) $(DEPLIBS)
	$(CC) $(EXTRA_FLAGS) $(LDFLAGS) -o $@ $(OBJ) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPLIBS): #
