EXTRA_CFLAGS = -I../../lib.ortho
IMAGERYLIB2  = ../../lib.ortho/libI.a


LIST = main.o\
	menu.o\
	run.o

$(BIN)/i.build.block: $(LIST) $(GISLIB) $(IMAGERYLIB) $(IMAGERYLIB2) $(VASKLIB)
	$(CC) $(EXTRA_CFLAGS) $(LDFLAGS) -o $@ $(LIST) $(IMAGERYLIB) $(IMAGERYLIB2) $(GISLIB) $(VASK)

$(GISLIB) $(IMAGERYLIB) $(IMAGERYLIB2) $(VASKLIB): #


