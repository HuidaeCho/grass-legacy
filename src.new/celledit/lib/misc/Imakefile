INSTLIBFLAGS = -m 0644

SRCS= test.c
OBJS= test.o

LIBSRCS = get_word.c open_infile.c open_outfile.c str_to_upper.c strip_blank.c \
       get_filename.c make_dir_list.c
	
LIBOBJS = get_word.o open_infile.o open_outfile.o str_to_upper.o strip_blank.o \
       get_filename.o make_dir_list.o


# CCOPTIONS = -pipe -I/usr/local/gnu/lib/gcc-includes

CC = /bin/cc

LIBDIR = ..
LIBS = $(LIBDIR)/libmisc.a

all:: libmisc.a lintlib install install.ln Test

NormalLibraryTarget(misc, $(LIBOBJS))
LintLibraryTarget(misc, $(LIBSRCS))

Test: $(LIBS)

SingleProgramTarget( Test, $(OBJS), $(LIBS),) 
InstallLibrary(misc, $(LIBDIR))
InstallLintLibrary(misc, $(LIBDIR))
