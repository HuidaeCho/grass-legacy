#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

SUBDIRS = misc brush grass Xutils

all:: 

LIBS: libmisc.a librbrush.a libgrass.a libXutils.a

CDEBUGFLAGS =

clean:: $(LIBS)
    /bin/rm -f $(LIBS)

MakeSubdirs($(SUBDIRS))
MakeLintLibSubdirs($(SUBDIRS))
MakeLintSubdirs($(SUBDIRS),install.ln,install.ln)

libmisc.a:
    (cd misc; make all)
libbrush.a:
    (cd brush; make all)
libgrass.a:
    (cd grass; make all)
libXutils.a:
    (cd Xutils; make all)
