INSTLIBFLAGS = -m 0644

SRCS = main.c
OBJS = main.o

LIBSRCS= brushio.c
LIBOBJS= brushio.o

LIBDIR = ..

CC = /bin/cc

LIBS= $(LIBDIR)/libbrush.a $(LIBDIR)/libmisc.a 

all:: libbrush.a lintlib install install.ln BrushTest
EXTRA_LOAD_FLAGS = -lm
BrushTest: $(LIBS)
NormalLibraryTarget(brush, $(LIBOBJS))
LintLibraryTarget(brush, $(LIBSRCS))

SingleProgramTarget(BrushTest, $(OBJS), $(LIBS),)
InstallLibrary(brush, $(LIBDIR))
InstallLintLibrary(brush, $(LIBDIR))
