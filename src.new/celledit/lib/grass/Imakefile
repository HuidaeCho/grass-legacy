INSTLIBFLAGS = -m 0644

SRCS = main.c
OBJS = main.o

LIBSRCS= get_data.c make_cell_path.c
LIBOBJS= get_data.o make_cell_path.o

LIBDIR = ..

LIBS= $(LIBDIR)/libgrass.a -L//home/grass3/src/libes/LIB.sun3 -lgis -lsegment -lm


all:: libgrass.a lintlib install install.ln GrassTest

NormalLibraryTarget(grass, $(LIBOBJS))
LintLibraryTarget(grass, $(LIBSRCS))

SingleProgramTarget(GrassTest, $(OBJS), $(LIBS),)
InstallLibrary(grass, $(LIBDIR))
InstallLintLibrary(grass, $(LIBDIR))
