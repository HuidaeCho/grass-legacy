	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(DLIB) $(DISPLAYLIB) ${RASTERLIB} $(GISLIB) $(VASK) -L$(XLIB) -lXaw -lXt -lX11 -lXmu $(MATHLIB)




