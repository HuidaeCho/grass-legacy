                     ARCH = mass1

                  GISBASE = /usr/grass3.2
                 UNIX_BIN = /usr/local/bin

         DEFAULT_DATABASE = /usr/grass3/data2
         DEFAULT_LOCATION = drum

            COMPILE_FLAGS = -O -flight
                  LDFLAGS = -s

                  TERMLIB = -ltermlib
                   CURSES = -lcurses $(TERMLIB)
