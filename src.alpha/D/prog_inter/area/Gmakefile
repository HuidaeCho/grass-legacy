	cc -o $@ $(LDFLAGS) $(OFILES) $(DISPLAYLIB) $(RASTERLIB) $(COORCNVLIB) $(GISLIB) $(DIGLIB) $(MATHLIB)

$(RASTERLIB): #
$(DISPLAYLIB): #
$(COORCNVLIB): #
$(GISLIB): #
