
FILES = gram.c lex.c makefile symtab.h symtab.c table.c main.c \
		flags.c engine.c misc.c reclass.c
OBJS = gram.o lex.o symtab.o table.o main.o \
		flags.o engine.o misc.o reclass.o

$(BIN)/Gbinfer: $(OBJS) $(GISLIB)
	$(CC) -o $@ $(LDFLAGS) $(OBJS) $(GISLIB) -ll $(MATHLIB)
	

count:
	@echo "Current number of lines:"
	@wc -l $(FILES)

$(OBJS): symtab.h
$(GISLIB): #
