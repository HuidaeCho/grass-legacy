PGM=d.frame
HOME=$(BIN_CMD)
all: \
	$(ETC)/frame.create \
	$(ETC)/frame.select \
	$(ETC)/frame.dumper \
	$(HOME)/$(PGM)

LIBES= $(D_LIB) $(DISPLAYLIB) $(RASTERLIB) $(GISLIB)

$(ETC)/frame.create: create.o $(LIBES)
	$(CC) $(LDFLAGS) -o $@ create.o $(LIBES) $(MATHLIB) $(XDRLIB)

$(ETC)/frame.select: select.o $(LIBES)
	$(CC) $(LDFLAGS) -o $@ select.o $(LIBES) $(MATHLIB) $(XDRLIB)

$(ETC)/frame.dumper: dumper.o $(LIBES)
	$(CC) $(LDFLAGS) -o $@ dumper.o $(LIBES) $(MATHLIB) $(XDRLIB)

$(HOME)/$(PGM): frame.o $(LIBES)
	$(CC) $(LDFLAGS) -o $@ frame.o $(LIBES) $(MATHLIB) $(XDRLIB)
	
$(RASTERLIB): #
$(DISPLAYLIB): #
$(D_LIB): #
$(GISLIB): #
