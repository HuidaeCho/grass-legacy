PGM=d.profile
EXTRA_CFLAGS	= $(DANOTHER_BUTTON)

OFILES = \
	DrawText.o\
        InitProfile.o\
        ExtractProf.o\
        PlotProfile.o\
        DumpProfile.o\
        What.o\
        Range.o\
        bnw_line.o\
	utils.o\
