PGM=d.vect.labels

LIBES= $(VECTLIB) $(DISPLAYLIB) $(RASTERLIB) $(GISLIB) $(DATETIMELIB)
DEPLIBS= $(DEPVECTLIB) $(DEPDISPLAYLIB) $(DEPRASTERLIB) $(DEPGISLIB) $(DEPDATETIMELIB)
LIST = main.o\
       do_labels.o	

EXTRACFLAGS=-Wall

all: $(BIN_CMD)/$(PGM)

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS) 
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(LIST): #
$(DEPLIBS): #
