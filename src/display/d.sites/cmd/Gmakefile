PGM=d.sites

OFILES = \
	draw_point.o \
	main.o

LIBES=$(DISPLAYLIB) $(RASTERLIB) $(GISLIB) $(GISLIB) $(DLGLIB) $(DATETIMELIB)
$(BIN_MAIN_CMD)/$(PGM): $(OFILES) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(RASTERLIB): #
$(DISPLAYLIB): #
$(GISLIB): #
$(DLGLIB): #
$(DATETIMELIB): #

draw_point.o: options.h
main.o: options.h
