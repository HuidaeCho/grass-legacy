PGM=d.sites

OFILES = \
	draw_point.o \
	main.o

LIBES=$(DISPLAYLIB) $(RASTERLIB) $(GISLIB) $(GISLIB) $(DLGLIB) $(DATETIMELIB)
DEPLIBS=$(DEPDISPLAYLIB) $(DEPRASTERLIB) $(DEPGISLIB) $(DEPGISLIB) $(DEPDLGLIB) $(DEPDATETIMELIB)
$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPRASTERLIB): #
$(DEPDISPLAYLIB): #
$(DEPGISLIB): #
$(DEPDLGLIB): #
$(DEPDATETIMELIB): #

draw_point.o: options.h
main.o: options.h
