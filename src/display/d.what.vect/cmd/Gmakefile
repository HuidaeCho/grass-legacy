PGM = d.what.vect
OFILES = \
	main.o \
	openvect.o\
	what.o


all: $(BIN_CMD)/$(PGM)

LIBES=$(VECTLIB) $(DISPLAYLIB) $(RASTERLIB) $(GISLIB)

$(BIN_CMD)/$(PGM): $(OFILES) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(RASTERLIB): #
$(DISPLAYLIB): #
$(GISLIB): #
$(VECTLIB): #
