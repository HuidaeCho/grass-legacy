PGM = d.what.vect
OFILES = \
	attr.o \
	main.o \
	openvect.o\
	what.o


all: $(BIN_CMD)/$(PGM)

DBMILIB = $(LIBDIR)/libdbmi.a
LIBES=$(VECTLIB) $(DISPLAYLIB) $(RASTERLIB) $(GISLIB) $(DBMILIB)

$(BIN_CMD)/$(PGM): $(OFILES) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(RASTERLIB): #
$(DISPLAYLIB): #
$(GISLIB): #
$(VECTLIB): #
