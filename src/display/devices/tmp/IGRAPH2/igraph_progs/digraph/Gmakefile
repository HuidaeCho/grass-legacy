
GRAPH =  -ltools_s -lc_s -lm
IGRAPHLIB =  ../../
EXTRA_CFLAGS	= -I$(IGRAPHLIB)

OFILES =  \
	curses.o \
	get_env.o \
	main.o \
	modify.o \
	pd_menu.o \
	print.o \
	store_env.o \
	tty.o 

$(BIN)/d.igraph: $(OFILES) 
	cc $(LDFLAGS) -o $@ $(OFILES) $(GRAPH)  $(GISLIB) -I$(LIBDIR) -I$(IGRAPHLIB) $(CURSES)

