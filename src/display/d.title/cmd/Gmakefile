PGM=d.title
OFILES = \
    fancy.o\
    main.o\
    normal.o

LIBES=$(DISPLAYLIB) $(RASTERLIB) $(GISLIB) 
$(BIN_CMD)/$(PGM): $(OFILES) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(GISLIB): #
$(RASTERLIB): #
$(DISPLAYLIB): #

main.o:	options.h
fancy.o:	options.h
normal.o:	options.h
