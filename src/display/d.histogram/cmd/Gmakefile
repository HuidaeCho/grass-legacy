PGM=d.histogram
OFILES = \
	get_stats.o\
	draw_slice.o\
	pie.o\
	bar.o\
	main.o

LIBES=$(DISPLAYLIB) $(RASTERLIB) $(GISLIB)
DEPLIBS=$(DEPDISPLAYLIB) $(DEPRASTERLIB) $(DEPGISLIB)

$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPRASTERLIB): #
$(DEPDISPLAYLIB): #
$(DEPGISLIB): #

command.o:	options.h
bar.o: 		bar.h dhist.h
pie.o: 		pie.h dhist.h
main.o:		options.h dhist.h
