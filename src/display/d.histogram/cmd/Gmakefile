PGM=d.histogram
OFILES = \
	get_stats.o\
	draw_slice.o\
	pie.o\
	bar.o\
	main.o

LIBES=$(DISPLAYLIB) $(RASTERLIB) $(GISLIB)

$(BIN_CMD)/$(PGM): $(OFILES) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(RASTERLIB): #
$(DISPLAYLIB): #
$(GISLIB): #

command.o:	options.h
bar.o: 		bar.h dhist.h
pie.o: 		pie.h dhist.h
main.o:		options.h dhist.h
