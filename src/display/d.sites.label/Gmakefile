LIBES = $(DISPLAYLIB) $(RASTERLIB) $(COORCNVLIB) $(GISLIB) $(DATETIMELIB) $(MATHLIB) $(XDRLIB)

LIST = draw_site_label.o main.o
PGM = $(BIN_ALPHA_CMD)/d.sites.label

$(PGM): $(LIST) $(LIBES)
	$(CC) $(LDFLAGS) -o $@  $(LIST) $(LIBES)
$(LIBES): #

clean:
	rm -rf OBJ.$(ARCH) *~
