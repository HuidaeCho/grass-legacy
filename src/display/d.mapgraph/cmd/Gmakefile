PGM=d.mapgraph
LIBES  = $(DISPLAYLIB) $(RASTERLIB) $(GISLIB) 
DEPLIBS= $(DEPDISPLAYLIB) $(DEPRASTERLIB) $(DEPGISLIB) 

LIST = \
	do_graph.o \
	graphics.o \
	read_line.o\
	main.o

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPLIBS): #

do_graph.o: options.h
main.o: options.h
read_line.o: options.h
