PGM=d.site.labels
OFILES = \
	do_labels.o \
	main.o

LIBES= $(DISPLAYLIB) $(RASTERLIB) $(GISLIB) $(DATETIMELIB)
DEPLIBS= $(DEPDISPLAYLIB) $(DEPRASTERLIB) $(DEPGISLIB) $(DEPDATETIMELIB)
$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPRASTERLIB): #
$(DEPDISPLAYLIB): #
$(DEPGISLIB): #
