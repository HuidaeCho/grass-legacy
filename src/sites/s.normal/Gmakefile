PGM=s.normal

CDHCDIR = ../cdhc
CDHCINC = ../cdhc
CDHCLIB = $(CDHCDIR)/libcdhc.a
 
EXTRA_CFLAGS =  -I$(CDHCINC)

LIBES = $(GISLIB) $(DATETIMELIB) $(CDHCLIB)
DEPLIBS= $(DEPGISLIB) $(DEPDATETIMELIB) $(CDHCLIB)

SOURCE  = \
	main.c \
	scancats.c

LIST  = \
	main.o \
	scancats.o

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS) 
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

main.o:       	$(CDHCINC)/cdhc.h 

$(CDHCLIB) : #
	$(GMAKE) $(CDHCDIR)

$(DEPGISLIB) : #
