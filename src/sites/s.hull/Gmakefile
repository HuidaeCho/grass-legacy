PGM = s.hull

LIST =  hull.o

LIBES= $(GISLIB) $(VECTLIB) $(DIG_ATTLIB) $(DATETIMELIB)
DEPLIBS= $(DEPGISLIB) $(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPDATETIMELIB)

all: $(BIN_CMD)/$(PGM)

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPGISLIB): #
