PGM=s.perturb

LIBES = $(GISLIB) $(DATETIMELIB)
DEPLIBS= $(DEPGISLIB) $(DEPDATETIMELIB)

LIST  = \
	main.o \
	myrng.o \
	zufall.o  \
	zufalli.o  \
	zufallsv.o  \
	zufallrs.o  \
        normal00.o   \
	normalen.o   \
	normalrs.o   \
	normalsv.o   \
	fische.o     
 
$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS) 
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

main.o:       	perturb.h 
readonesite.o:	perturb.h
