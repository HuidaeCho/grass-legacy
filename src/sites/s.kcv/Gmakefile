LIBES = $(GISLIB) $(DATETIMELIB)
PGM=s.kcv

LIST  = \
	main.o \
	dreset.o \
	readsite.o \
	histo.o \
	utils.o

$(BIN_CMD)/$(PGM): $(LIST) $(LIBES) 
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

main.o:       	kcv.h version.h
readsite.o:	kcv.h version.h
histo.o:	version.h
dreset.o:	kcv.h version.h
utils.o:	kcv.h version.h
