PGM = site_mask
I = ../Include
EXTRA_CFLAGS  = -I$I
SITELIB = ../lib.a
SITE = $(ETC)/sites

LIST =  main.o\
	get_mask.o\
	makemask.o\
	mask_sites.o\
	new_screen.o

$(SITE)/$(PGM): $(LIST) $(SITELIB) $(GISLIB)
		$(CC) ${LDFLAGS} -o $(SITE)/$(PGM) $(LIST) $(SITELIB) $(GISLIB) $(MATHLIB) $(XDRLIB) $(VASK)

$(LIST):	$I/site.h

$(SITELIB):	#
$(GISLIB):	#
