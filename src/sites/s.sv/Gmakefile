PGM=s.sv

LIST=main.o readsite.o cdpath.o save.o nbins.o angleok.o plthgpt.o

LIBES=$(GISLIB) $(RASTERLIB) $(DATETIMELIB)

$(BIN_CONTRIB_CMD)/$(PGM): $(LIST) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

main.o		: sv.h 
readsites.o	: sv.h 
save.o		: sv.h 
plthgpt.o	: sv.h 
