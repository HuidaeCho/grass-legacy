PGM=s.probplt

LIST=probplot.o cdpath.o zstats.o pltsqq.o save.o

LIBES=$(GISLIB) $(DATETIMELIB) 
DEPLIBS=$(DEPGISLIB) $(DEPDATETIMELIB) 

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST)  $(LIBES) $(MATHLIB) $(XDRLIB)

pltsqq.o	: probplot.h 
probplot.o	: probplot.h 
save.o		: probplot.h 
