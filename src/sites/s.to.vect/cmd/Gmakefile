PGM = s.to.vect

LIST =  main.o\
	new_screen.o\
	head_info.o

LIBES=$(GISLIB) $(VECTLIB) $(DIG_ATTLIB) $(VASKLIB) $(DATETIMELIB)
DEPLIBS=$(DEPGISLIB) $(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPVASKLIB) $(DEPDATETIMELIB)

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST)  $(LIBES) $(CURSES) $(MATHLIB) $(XDRLIB)

