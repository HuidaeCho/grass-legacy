PGM=s.qcount
 
TUTORIAL = s.qcount-tutorial

SOURCE = \
	count.c \
	findquads.c \
	indices.c \
	main.c

LIST = \
	count.o \
	findquads.o \
	indices.o \
	main.o

LIBES= $(GISLIB) $(DATETIMELIB) 
DEPLIBS= $(DEPGISLIB) $(DEPDATETIMELIB) 

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

count.o:  quaddefs.h 
findquads.o: quaddefs.h 
indices.o:  quaddefs.h 
main.o:  quaddefs.h 
