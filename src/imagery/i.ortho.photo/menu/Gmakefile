PGM      = i.ortho.photo

EXTRA_CFLAGS = -I../libes
IMAGERYLIB2  = ../libes/libortho.a


LIST =  menu.o\
	run.o

LIBES= $(IMAGERYLIB) $(IMAGERYLIB2) $(GISLIB) $(VASKLIB)
DEPLIBS= $(DEPIMAGERYLIB) $(IMAGERYLIB2) $(DEPGISLIB) $(DEPVASKLIB)

$(BIN_INTER)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(CURSES) $(MATHLIB) $(XDRLIB)

$(DEPLIBS): #


