PGM=i.fft
PGMI=i.ifft

OBJS = fftmain.o orig_wind.o save_fft.o fft_colors.o

INVOBJS = ifftmain.o orig_wind.o

LIBES= $(GISLIB) $(GMATHLIB)

all: $(BIN_CMD)/$(PGM) $(BIN_CMD)/$(PGMI)

$(BIN_CMD)/$(PGM): $(OBJS) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(OBJS) $(LIBES) $(XDRLIB) $(FFTWLIB) $(MATHLIB)


$(BIN_CMD)/$(PGMI): $(INVOBJS) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(INVOBJS) $(LIBES) $(XDRLIB) $(FFTWLIB) $(MATHLIB)

$(LIBES): #
