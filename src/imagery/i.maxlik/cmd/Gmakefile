PGM=i.maxlik
LIST = main.o\
	open.o\
	classify.o\
	hist.o\
	invert.o

LIBES=$(IMAGERYLIB) $(GISLIB) $(VASKLIB)
DEPLIBS=$(DEPIMAGERYLIB) $(DEPGISLIB) $(DEPVASKLIB)
$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(LIST): global.h

$(DEPIMAGERYLIB): #
$(DEPGISLIB): #
$(DEPVASKLIB): #
