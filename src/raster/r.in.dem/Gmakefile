PGM    = r.in.dem
HOME   = $(BIN_CONTRIB_CMD)

EXTRA_CFLAGS = 
OBJ	= \
	main.o \
	dem_read.o \
	trans_ll2u.o \
	bintree.o

INCLUDE = -I$(INCLUDE_DIR)
LIBES	= $(COORCNVLIB) $(GISLIB) $(VECTLIB) $(DATETIMELIB)

$(HOME)/$(PGM): $(OBJ) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(OBJ) $(LIBES) $(MATHLIB) $(XDRLIB)

$(COORCNVLIB): #
$(GISLIB): #
$(VECTLIB): #
$(DATETIMELIB): #
