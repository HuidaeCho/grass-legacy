PGM=r.out.png
EXTRA_CFLAGS=$(PNGINC)
LIBES = $(PNGLIB) -lpng -lz

LIST= r.out.png.o

$(BIN_CONTRIB_CMD)/$(PGM): $(LIST) $(GISLIB)
	$(CC) $(LDFLAGS) $(EXTRA_CFLAGS) -o $@ $(LIST) $(GISLIB) $(LIBES) $(EXTRALIB) $(MATHLIB) $(XDRLIB)

$(GISLIB): #
