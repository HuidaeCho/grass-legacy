PGM = r.random.surface

#These functions are obtained by specifying -ldl as an option to ld(1).

OBJS =	cpfilter.o gennorm.o makepp.o decay.o init.o save.o \
	calcsd.o digits.o calcsurf.o gasdev.o main.o random.o \
	dd.o makebigf.o zero.o

$(BIN_CMD)/$(PGM): $(OBJS) $(GISLIB)
	$(CC) $(LDFLAGS) -o $@ $(OBJS) $(GISLIB) $(DLLIB) $(MATHLIB) $(XDRLIB)

$(OBJS): ransurf.h

$(GISLIB): #
