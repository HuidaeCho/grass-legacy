PGM = r.le.trace

OFILES = main.o user_input.o

LIBES = $(DISPLAYLIB) $(RASTERLIB) $(VECTLIB) $(VASKLIB) $(GISLIB)
DEPLIBS= $(DEPDISPLAYLIB) $(DEPRASTERLIB) $(DEPVECTLIB) $(DEPVASKLIB) $(DEPGISLIB)

$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(VASK) $(MATHLIB) $(XDRLIB)

$(DEPLIBS): #
