	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPGISLIB): #

$(OFILES):	extr_areas.h
