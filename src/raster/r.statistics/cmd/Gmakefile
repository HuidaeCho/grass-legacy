PGM    = r.statistics

METHODS = \
	o_average.o\
	o_distrib.o\
	o_mode.o\
	o_adev.o\
	o_sdev.o\
	o_var.o\
	o_skew.o\
	o_kurt.o\
	o_median.o\
	o_min.o\
	o_max.o\
	o_sum.o

OTHER =\
	main.o\
	read_stats.o\
	write_rec.o


LIST   = $(METHODS) $(OTHER)

$(BIN_CMD)/$(PGM): $(LIST) $(GISLIB)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(GISLIB) $(MATHLIB) $(XDRLIB)

main.o: method.h

$(GISLIB): #
