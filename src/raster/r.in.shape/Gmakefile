PGM = r.in.shape

SHAPELIB_DIR=$(SRC)/libes/vect32_64/shapelib-1.2.8
SHAPELIB=$(SHAPELIB_DIR)/libshape.a

HOME = $(BIN_CONTRIB_CMD)
EXTRA_CFLAGS = -I../../libes/vect32_64/shapelib-1.2.8/
LIST = 	main.o


$(HOME)/$(PGM): $(LIST) $(GISLIB) $(DATETIMELIB)
	$(GMAKE) $(SHAPELIB_DIR)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(VECTLIB) $(VASK) \
	$(MATHLIB) $(DATETIMELIB) $(XDRLIB) $(SHAPELIB) $(VECTLIB_REAL) $(DIG2LIB) \
	$(GISLIB) $(XDRLIB) $(MATHLIB)

	
$(VECTLIB): #
$(GISLIB): # 
