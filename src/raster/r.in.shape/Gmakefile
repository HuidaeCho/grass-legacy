LIST = 	main.o


$(HOME)/$(PGM): $(LIST) $(GISLIB) $(DATETIMELIB)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(VECTLIB) $(VASK) \
	$(MATHLIB) $(DATETIMELIB) $(XDRLIB) $(SHAPELIB) $(VECTLIB_REAL) $(DIG2LIB) \
	$(GISLIB) 

	
$(VECTLIB): #
