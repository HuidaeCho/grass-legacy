PGM = r.mfilter

LIST = \
	main.o\
	getfilt.o\
	getrow.o\
	apply.o\
	execute.o\
	perform.o\
	uppercase.o

$(BIN_CMD)/$(PGM): $(LIST) $(DEPGISLIB) $(DEPROWIOLIB)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(GISLIB) $(ROWIOLIB) $(MATHLIB) $(XDRLIB)

$(LIST): filter.h glob.h

$(DEPGISLIB): # in case library changes
$(DEPROWIOLIB): # in case library changes
