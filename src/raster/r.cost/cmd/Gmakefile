PGM = r.cost
LIST =  main.o \
	btree.o \
	memory.o

LIBES=$(GISLIB) $(DISPLAYLIB) $(SEGMENTLIB) $(DATETIMELIB)
DEPLIBS=$(DEPGISLIB) $(DEPDISPLAYLIB) $(DEPSEGMENTLIB) $(DEPDATETIMELIB)

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPGISLIB): #
$(DEPDISPLAYLIB): #
$(DEPSEGMENTLIB): #

delete.o: cost.h
insert.o: cost.h
main.o: cost.h
main.o: stash.h
