#  %W%  %G%


all: Dsave.slide Drestore.slide

Dsave.slide: $(BIN)/Dsave.slide
$(BIN)/Dsave.slide: main.save.o $(DISPLAYLIB) $(RASTERLIB) $(GISLIB)
	$(CC) $(LDFLAGS) -o $@ main.save.o $(DISPLAYLIB) $(RASTERLIB) $(GISLIB) $(RASTERLIB) 

Drestore.slide: $(BIN)/Drestore.slide
$(BIN)/Drestore.slide: main.restore.o $(DISPLAYLIB) $(RASTERLIB) $(GISLIB)
	$(CC) $(LDFLAGS) -o $@ main.restore.o $(DISPLAYLIB) $(RASTERLIB) $(GISLIB) $(RASTERLIB) 

$(RASTERLIB): #
$(DISPLAYLIB): #
$(GISLIB): #
