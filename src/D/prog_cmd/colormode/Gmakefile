OFILES = \
	colormode.o \
	command.o

Dcolormode: $(BIN)/Dcolormode

$(BIN)/Dcolormode: $(OFILES) $(DISPLAYLIB) $(RASTERLIB) $(GISLIB)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(DISPLAYLIB) $(RASTERLIB) $(GISLIB)

$(RASTERLIB): #
$(DISPLAYLIB): #
$(GISLIB): #

colormode.o:	options.h
command.o:	options.h
