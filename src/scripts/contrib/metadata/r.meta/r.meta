#
# Check to see if GRASS is running
#
if test "$GISBASE" = ""; then
 echo "You must be in GRASS GIS to run this program." >&2
 exit 1
fi   
     
eval `g.gisenv`
: ${GISBASE?} ${GISDBASE?} ${LOCATION_NAME?} ${MAPSET?}
LOCATION=$GISDBASE/$LOCATION_NAME/$MAPSET

#----------------------------------------------------------------------------
#
#  Evaluate arguments
#
if [ $# != 1 ]
then clear
    echo
    echo "Usage: `basename $0 `  mapname "
    echo "          <or>        "
    echo "       `basename $0 ` m=mapname "
    echo "          <or>        "
    echo "       `basename $0 ` map=mapname "
    echo
    echo " Where \"mapname\" is a raster map in your current mapset. "
    echo 
    echo " Please try again."
    echo
    exit 1
fi
#
#  parse input arguments
#
case $1 in
	map=*) META=`echo $1 | sed s/map=//` ;;
	m=*)     META=`echo $1 | sed s/m=//` ;;
	see=*) SEE=1; SEEFILE=`echo $1 | sed s/see=//` ;;
	s=*)     SEE=1; SEEFILE=`echo $1 | sed s/s=//` ;;
	*)                    META=`echo $1` ;;
esac
echo 
echo

# Define some variables -----
#
# METADIR="$LOCATION/metadata"
# METAFILE="$METADIR/$META"
#
eval `g.findfile element=metadata file=$META`
METADIR="$GISDBASE/$LOCATION_NAME/$mapset/metadata"
METAFILE="$file"
NEW=0

#
# User only wants to non-interactively display the metadata file
#

if [ "$SEE" = 1 ]
then
	eval `g.findfile element=metadata file=$SEEFILE`
	if [ ! "$file" ]
	then
		echo metadata file "$SEEFILE" not found
		echo
	else
		cat "$file" | more
	fi
exit 1
fi
	
if [ ! "$file" ]
then
        NEW=1
	METADIR="$LOCATION/metadata"
	METAFILE="$METADIR/$META"
fi

if [ "$NEW" = 1 ]
then

# Check to see if $LOCATION/metadata already exists
#
#
#element=`ls $LOCATION | grep metadata`
if [ "$element" != metadata ]
then clear
    echo
    echo "        ***** METADATA DIRECTORY OPTION MENU ***** "
    echo
    echo
    echo  "The directory metadata at $LOCATION does not exist "
    echo  
    echo -n "Create?  Type Y to continue, anything else to quit:   "
    read ans
      case $ans in 
          y|yes|Y|YES|Yes)        mkdir $LOCATION/metadata ;;
          *)                                        ans="N" ;;
      esac
    if [ "$ans" = N  ]
    then echo
    echo "        Let's clarify this: "
    echo
    echo "        You MUST have a metadata directory at"
    echo
    echo "        $LOCATION if you wish to continue."
    echo
    echo "        That's were the metadata file will reside. "
    echo
    echo -n "Create?  Type Y to continue, anything else to quit:   "
    read ans
      case $ans in 
          y|yes|Y|YES|Yes)        mkdir $LOCATION/metadata ;;
              *)                                   ans="N" ;;
      esac
    echo
    exit 1
    fi
fi
fi
#

#---------------------------------------------------------------------
#
# What to do if METAFILE already exists.
#
if [ "$NEW" = 0 ]
then
	clear
        echo
	echo "     ***** MAIN OPTIONS MENU *****"
        echo 
        echo
	echo "    METADATA file $META already exists !"
        echo
        echo "Do you want to:"
        echo "               (S) ee "
        echo "               (D) elete "
        echo "               (E) dit   "
        echo
        echo " the existing METADATA file $META in mapset: $mapset ?"
        echo
        echo "         or    (Q) uit  " 
        echo
        echo -n "Enter your choice.... "
        read ans1
	case "$ans1" in
          s|S|see|SEE|See) cat $METAFILE | more; sleep 2; exit 1 ;;
          d|D|delete|DELETE|Delete)     ans2="D" ;;
          e|E|edit|EDIT|Edit)           ans2="E" ;;
          q|Q|quit|Quit|QUIT)           exit 1  ;;
          *)                           continue ;;
	esac
        if [ "$ans2" = D ]
        then clear
		#
		# See if the metadata file is owned by the user
		#
		if [ "$MAPSET" != "$mapset" ]
		then
			echo "You do not own the metadata file $META"

			echo "You cannot edit or delete a metadata file that"
			echo "you do not own."
			exit 1;
		else
		echo
		echo
		echo "     ***** DELETE OPTIONS MENU *****"
		echo
		echo
        	echo "           ARE YOU SURE ?"
		echo "                   ^^^^"
        	echo
        	echo "Enter (Y) es to delete, anything else to return "
        	echo
        	echo -n "to the Main Options Menu.... "
        	read ans3
        	case "$ans3" in
        	   y|yes|Y|YES|Yes)       rm $METAFILE 
			echo
			echo
			echo "You just deleted your METADATA file $META"
			echo
			echo "Do you want to create a new METADATA file?"
			echo
       			echo -n "Enter (Y) es to go on, anything else to quit."
			read ans
			case $ans in 
       			y|yes|Y|YES|Yes)        break  ;;
			*)                     exit 1  ;;
			esac     ;;
           	*)                  continue ;;
        	esac
		fi
        fi
	if [ "$ans2" = E ]
	then clear
		#
		# See if the metadata file is owned by the user
		#
		if [ "$MAPSET" != "$mapset" ]
		then
			echo "You do not own the metadata file $META"
			echo
			echo "You cannot edit or delete a metadata file that"
			echo "you do not own."
			echo 
			echo
			echo "Would you like to see the file anyway?"
			echo
       			echo -n "Enter (Y) es to view the metadata file, anything else to quit."
			read ans
			case $ans in 
       			y|yes|Y|YES|Yes)  cat $METAFILE | more; sleep 2; continue ;;
			*)                     exit 1  ;;
			esac
			exit 1
		else

		echo
		echo "     ***** EDITING OPTIONS MENU *****"
		echo
		echo
		echo "If you only have a few corrections/additions, "
		echo
		echo "It will be easier to do that work in a text editor"
		echo
		echo "Do you want to Continue (using vi to edit) or Quit?"
		echo
		echo "Enter (C) to continue or (Q) to quit... "
		echo
		echo -n "or any other key to return to previous menu.... "
		read ans4
		case "$ans4" in
        	  q|Q|quit|Quit|QUIT)   exit 1  ;;
		  c|C|continue|Continue|CONTINUE)    
				vi $METAFILE
 				echo
 				echo
				echo " Editing of metafile $META now finished "
 				echo
				echo "You will now be returned to the MAIN options menu"
				sleep 5
				continue ;;
	  	*)            	continue ;;
		esac
		break
		fi
	fi
fi
