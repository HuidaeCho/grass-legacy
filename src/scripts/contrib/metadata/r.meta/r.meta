	then
	CATNUM=`head -1 "$CATS" | awk '{print $2+1}'`
	else
	NODATA=`tail +5 "$CATS" | head -1 | awk -F: '{print $1}'`
	if [ $NODATA = 0 ]
		then
		CATNUM=`tail +5 "$CATS" | wc -l`
		else
		CATNUM=`tail +4 "$CATS" | wc -l`
	fi
fi
