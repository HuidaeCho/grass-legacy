ZONE=`tail +2 "$CELLHD" | head -1 | awk '{print $2}'`
N=`tail +3 "$CELLHD" | head -1 | awk '{print $2}'`
S=`tail +4 "$CELLHD" | head -1 | awk '{print $2}'`
OLDORNEW=`awk '{} END {print NR}' $CELLHD `

if [ $OLDORNEW = 12 ] 
        then 
        W=`tail +5 "$CELLHD" | head -1 | awk '{print $2}'`
        E=`tail +6 "$CELLHD" | head -1 | awk '{print $2}'`
        NS=`tail +10 "$CELLHD" | head -1 | awk '{print $3}'` 
        EW=`tail +9 "$CELLHD" | head -1 | awk '{print $3}'` 
        COL=`tail +7 "$CELLHD" | head -1 |awk '{print $2}'` 
        ROW=`tail +8 "$CELLHD" | head -1 |awk '{print $2}'` 
        TOTAL=`expr "$COL" "*" "$ROW"`
        else
        E=`tail +6 "$CELLHD" | head -1 | awk '{print $2}'`
        W=`tail +5 "$CELLHD" | head -1 | awk '{print $2}'`
        NS=`tail +7 "$CELLHD" | head -1 | awk '{print $3}'`
        EW=`tail +8 "$CELLHD" | head -1 | awk '{print $3}'`
        COL=`echo "$N $S $NS" | awk '{print (($1-$2)/$3)}'`
        ROW=`echo "$E $W $EW" | awk '{print (($1-$2)/$3)}'`
        TOTAL=`echo "$COL $ROW" | awk '{print ($1*$2)}'`
fi
