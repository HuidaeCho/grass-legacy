EXTRA_CFLAGS = $(XCFLAGS) $(XINCPATH) $(XMINCPATH)
LIBS = $(XGILIB) $(XGDLIB) $(GISLIB) $(XMLIBPATH) $(XMLIB) $(XTLIBPATH) \
	$(XTLIB) $(XLIBPATH) $(XLIB) $(XEXTRALIBS)

all:: $(BIN)/xgmapcalc

OBJS = \
	xgmapcalc.o 

$(BIN)/xgmapcalc: $(OBJS) $(XGILIB) $(GISLIB)
	$(CC) -o $@ $(LDFLAGS) $(XLDFLAGS) $(EXTRA_LDFLAGS) $(OBJS) $(LIBS)

$(XGILIB) (GISLIB): #
