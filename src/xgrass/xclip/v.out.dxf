program:"v.out.dxf"

title:"Convert GRASS Vector Files to DXF"

commandstring:"input=(iname) output=(oname)"

description:"v.out.dxf converts GRASS vector format files to DXF format files."

help:"This program allows you to convert GRASS vector format files to DXF format files."
helpwidgetref:"08.mapdev/08.05.im.ex:Importing and Exporting Data"

{
    parameter iname
	description:"GRASS vector ASCII input file:"
	type:database_element:vector
	help:"Enter the name of the GRASS vector ASCII file to be used as input."
	optional:false
	input:true
	;

    parameter oname
	description:"DXF output file:"
	type:character
	help:"Enter the name of the DXF output file."
	optional:false
	input:true
	;
}
