program:"r.traj.data"

title:"Review Ammunition and Weapon Data Base"

commandstring:"[-(a)(w)]"

description:"r.traj.data reviews the ammunition and weapon data base used by r.traj."

help:"This program allows you to review the ammunition and weapon data base used by r.traj."

{

    flag a
	key:"a"
	description:"Print list of ammunition types."
	help:"Click this button to print a listing of the ammunition types usable by r.traj." 
	;

    flag w
	key:"w"
	description:"Print list of weapons."
	help:"Click this button to print a listing of the weapons usable by r.traj." 
	;
}
