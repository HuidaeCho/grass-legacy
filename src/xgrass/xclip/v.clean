program:"v.clean"

title:"Clean Dead Lines"

commandstring:"map=(mname)"

description:"v.clean will go through a vector file and remove all dead lines in the file."

help:"Use this command to clean dead lines from a vector file."

{
    parameter mname
	description:"Existing vector file to clean:"
	type:database_element:vector
	input:true
	help:"Enter the name of the vector file to clean."
	optional:false
	;
}

