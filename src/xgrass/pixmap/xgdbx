#!/bin/sh
PATH=/grass.src/4.1/sun4/bin:/grass.src/4.1/sun4/scripts:/grass.src/4.1/sun4/garden/bin:$PATH
export PATH
if test "$XAPPLRESDIR" = ""; then
XAPPLRESDIR=/grass.src/4.1/sun4/etc/app-defaults
export XAPPLRESDIR
fi
GISBASE=/grass.src/4.1/sun4
export GISBASE
XGRASSLIBDIR=/grass.src/4.1/sun4/etc
export XGRASSLIBDIR
XGRASSHELPDIR=/grass.src/4.1/sun4/etc/xhelp
export XGRASSHELPDIR
exec dbx /grass.src/4.1/sun4/bin/pixmap
