PGM = nullTest
OBJ = main.o
SOURCES = main.c

LIBES = $(GISLIB)
DEPLIBS= $(DEPGISLIB)

$(PGM): $(OBJ) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OBJ) $(LIBES) $(XDRLIB)

$(DEPGISLIB): #
