
RESET = reset.o
SCAN = scan.o
INCR = incr.o
SUB = sub.o
LOCAL = local.o


all: scan reset incr sub try local

sub: $(SUB) $(DEPDATETIMELIB)
	$(CC) $(LDFLAGS) -o $@ $(SUB) $(DATETIMELIB)

incr: $(INCR) $(DEPDATETIMELIB)
	$(CC) $(LDFLAGS) -o $@ $(INCR) $(DATETIMELIB)

scan: $(SCAN) $(DEPDATETIMELIB)
	$(CC) $(LDFLAGS) -o $@ $(SCAN) $(DATETIMELIB)

reset: $(RESET) $(DEPDATETIMELIB)
	$(CC) $(LDFLAGS) -o $@ $(RESET) $(DATETIMELIB)

local: $(LOCAL) $(DEPDATETIMELIB)
	$(CC) $(LDFLAGS) -o $@ $(LOCAL) $(DATETIMELIB)


TRY = try.o normalize.o
try: $(TRY) $(DEPDATETIMELIB)
	$(CC) $(LDFLAGS) -o $@ $(TRY) $(DATETIMELIB)
