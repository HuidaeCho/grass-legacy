EXTRA_CFLAGS = $(PICFLAGS)

SLIBDEPS = $(LINKMLIB)

OBJ = \
	bitmap.o\
	sparse.o

all: $(DEPBITMAPLIB)

$(DEPBITMAPLIB): $(OBJ)
	$(SLIBRULE)

$(OBJ): #linkm.h

main: main.o $(DEPBITMAPLIB) $(DEPLINKMLIB)
	$(CC) $(LDFLAGS) main.o $(BITMAPLIB) $(LINKMLIB) -o main

smain: smain.o $(DEPBITMAPLIB) $(DEPLINKMLIB)
	$(CC) $(LDFLAGS) smain.o $(BITMAPLIB) $(LINKMLIB) -o smain

$(DEPLINKMLIB): #
