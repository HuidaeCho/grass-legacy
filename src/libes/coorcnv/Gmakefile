OBJ = \
    geocen1.o\
    geocen2.o\
    spheroid.o\
    tm.o\
    utm.o\
    format_ll.o\
    scan_ll.o\
    datum.o\
    molodens.o
    
all: 	version.h \
	$(COORCNVLIB) \
	$(GISLIB) \
	$(ETC)/datum.table

$(COORCNVLIB): $(OBJ)
	$(AR)

$(ETC)/datum.table: datum.table
	rm -f $@
	cp $? $@
	chmod +r $@

LIBES = $(COORCNVLIB) $(GISLIB)

version.h: $(VERSION_FILE)
	echo 'static char *VERSION_STRING = "@(#) GRASS $(VERSION_NUMBER) ($(VERSION_DATE))";' > $@

