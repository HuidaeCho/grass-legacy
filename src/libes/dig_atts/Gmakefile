OBJ = \
	atts_init.o \
	read_att.o \
	write_att.o 

ATTS = $(LIBDIR)/dig_atts.h

all: $(ATTS) $(DIG_ATTLIB) 

$(DIG_ATTLIB): $(OBJ)
	$(AR)

$(ATTS): dig_atts.h
	rm -f $@
	cp $? $@
	chmod +r $@


#  Program to test the atts functions, also example of how to use

test_atts:  t_atts.o
	rm -f test_atts
	$(CC)  $(LDFLAGS) -o $@  t_atts.o $(DIG_ATTLIB) $(GISLIB)

tt:  tt.o
	rm -f tt
	$(CC)  $(LDFLAGS) -o $@  tt.o $(DIG_ATTLIB) $(GISLIB)

$(OBJ): dig_atts.h

