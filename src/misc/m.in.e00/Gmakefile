PGM = m.in.e00
LIST = 	main.o \
	e00read.o \
	info.o \
	raster.o \
	projection.o \
	label.o \
	vector.o

LIBES=$(VECTLIB) $(GISLIB) $(VASKLIB) $(DATETIMELIB)
DEPLIBS=$(DEPVECTLIB) $(DEPGISLIB) $(DEPVASKLIB) $(DEPDATETIMELIB)
$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

tstinfo: info.o
	$(CC) $(LDFLAGS) -o $@ info.o $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPGISLIB): # 
