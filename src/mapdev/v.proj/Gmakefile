PGM=v.proj
EXTRA_CFLAGS=-g

OFILES = \
	main.o setenv.o 

SRCS = \
	main.c setenv.c

LIBES=$(GPROJLIB) $(VECTLIB) $(DIG_ATTLIB) $(GISLIB) $(LIBPROJSUPP) $(LIBPROJ)
DEPLIBS=$(DEPGPROJLIB) $(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPGISLIB) $(LIBPROJSUPP) $(LIBPROJ)
$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPGPROJLIB) $(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPGISLIB): # 
