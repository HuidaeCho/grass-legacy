PGM = v.what
OFILES = \
	main.o \
	openvect.o\
	what.o

LIBES=$(VECTLIB) $(VECTLIB_REAL) $(DIG2LIB) $(DISPLAYLIB) $(RASTERLIB) $(GISLIB)
DEPLIBS=$(DEPVECTLIB) $(DEPVECTLIB_REAL) $(DEPDIG2LIB) $(DEPDISPLAYLIB) $(DEPRASTERLIB) $(DEPGISLIB)

$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPRASTERLIB): #
$(DEPDISPLAYLIB): #
$(DEPGISLIB): #
$(DEPVECTLIB): #
