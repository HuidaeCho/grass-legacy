PROG_CMD = $(BIN_CMD)/v.prune 
PROG_INT = $(BIN_INTER)/v.prune

all:  $(PROG_CMD) $(PROG_INT)
	

#  interactive version

$(PROG_INT): prune.o $(VECTLIB) $(GISLIB)
	$(CC) $(LDFLAGS) -o $@ prune.o $(VECTLIB) $(GISLIB) $(MATHLIB) $(XDRLIB)

#  command line version

$(PROG_CMD): v.prune.o $(VECTLIB) $(GISLIB) $(DIG_ATTLIB)
	$(CC) $(LDFLAGS) -o $@ v.prune.o $(VECTLIB) $(DIG_ATTLIB) $(GISLIB) $(MATHLIB) $(LINKMLIB) $(XDRLIB)

$(VECTLIB) $(GISLIB) $(DIG_ATTLIB): #
