EXTRA_CFLAGS = $(VECT_INCLUDE)

PGM =	v.out.shape

OBJ =	Vexport_arc.o\
	write_areas.o\
	write_lines.o\
	write.o\
	writ_a_lines.o


$(BIN_ALPHA_CMD)/$(PGM): $(OBJ) $(VECTLIB) $(GISLIB) $(DIG_ATTLIB)
	$(GMAKE) gen2shp-0.2.2
	$(GMAKE) shapelib-1.2.7
	$(CC) -o $@  $(LDFLAGS) $(OBJ) $(VECTLIB) $(GISLIB) $(DIG_ATTLIB) $(MATHLIB) $(XDRLIB)

$(VECTLIB): #
$(DIG_ATTLIB): #
$(GISLIB): #
