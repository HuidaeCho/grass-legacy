CMD_CFILES = Build_cmd.c\
      Col.c\
      CountLines.c\
      CountCats.c\
      FindAtt_Cat.c\
      FindIDColumn.c\
      A_labs_cmd.c\
      GenToDig.c\
      L_labs_cmd.c\
      DO_txt_file.c\
      process_inp.c\
	  main_cmd.c

CMD_OBJ = Build_cmd.o\
      Col.o\
      CountLines.o\
      CountCats.o\
      FindAtt_Cat.o\
      FindIDColumn.o\
      A_labs_cmd.o\
      GenToDig.o\
      L_labs_cmd.o\
      DO_txt_file.o\
      process_inp.o\
