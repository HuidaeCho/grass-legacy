PGM=v.cadlabel

SOURCE = v.cadlabel.c \
	interval.c \
	label_lines.c \
	line_center.c

OBJ = v.cadlabel.o \
	interval.o \
	label_lines.o \
	line_center.o 

LIBES=$(VECTLIB) $(GISLIB) $(DIG_ATTLIB)
DEPLIBS=$(DEPVECTLIB) $(DEPGISLIB) $(DEPDIG_ATTLIB)
$(BIN_CMD)/$(PGM): $(OBJ) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OBJ) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPVECTLIB) $(DEPDIG_ATTLIB) $(DEPGISLIB): #
