PGM =	v.out.arc

OBJ =	main.o\
	write_areas.o\
	write_lines.o\
	write.o\
	writ_a_lines.o

LIBES=$(VECTLIB) $(GISLIB) $(DIG_ATTLIB)
DEPLIBS=$(DEPVECTLIB) $(DEPGISLIB) $(DEPDIG_ATTLIB)

$(BIN_CMD)/$(PGM): $(OBJ) $(DEPLIBS)
	$(CC) -o $@  $(LDFLAGS) $(OBJ) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPVECTLIB): #
$(DEPDIG_ATTLIB): #
$(DEPGISLIB): #
