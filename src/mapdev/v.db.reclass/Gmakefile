PGM=v.db.reclass

LIBES = $(VECTLIB) $(DIG_ATTLIB)\
	$(DBMILIB) $(VECTLIB_REAL) $(DIG2LIB)\
	$(GISLIB) 

DEPLIBS= $(DEPVECTLIB) $(DEPDIG_ATTLIB)\
	$(DEPDBMILIB) $(DEPVECTLIB_REAL) $(DEPDIG2LIB)\
	$(DEPGISLIB) 


LIST = main.o\
	input.o\
	key_data.o\
	reclass.o

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(DEPLIBS): #
