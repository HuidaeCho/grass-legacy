PGM=v.alabel

# include VECTLIBFLAGS for the PORTABLE_3 flag
EXTRA_CFLAGS    = $(VECTLIBFLAGS)

LIBES=$(VECTLIB) $(GISLIB) $(LINKMLIB) $(DIG_ATTLIB)

$(BIN_CMD)/$(PGM): main.o  $(LIBES)
	$(CC) $(LDFLAGS) -o $@ main.o $(LIBES) $(MATHLIB) $(XDRLIB)

$(VECTLIB): #
$(GISLIB): #
$(LINKMLIB): #
$(DIG_ATTLIB): #
