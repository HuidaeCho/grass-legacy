PGM = v.in.shape


EXTRA_CFLAGS = $(VECT_INCLUDE)

LIST = 	basename.o \
	category.o \
	polygon.o \
	main.o \
	shpopen.o \
	dbfopen.o \
	import.o \
	lines.o \
	vmap_import.o


LIBES=$(VECTLIB) $(GISLIB) $(LINKMLIB) $(DATETIMELIB) $(VASKLIB) $(BTREELIB)

$(BIN_CMD)/$(PGM): $(LIST) $(LIBES)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(CURSES) $(MATHLIB) $(XDRLIB)

$(VECTLIB): #
$(GISLIB): # 
