PGM=v.apply.census
LIBES = $(LIBDIR)/libcoorcnv.a $(GISLIB) $(VECTLIB) $(DIG_ATTLIB)
DOCS = $(ETC)/census.docs 

$(BIN_CMD)/$(PGM): v.apply.cen.o $(LIBES)
	$(CC) $(LDFLAGS) -o $@ v.apply.cen.o $(LIBES) $(MATHLIB) $(XDRLIB)
	-mkdir $(DOCS)
	cp stf1_matrix $(DOCS)
	cp pl94_matrix $(DOCS)
	cp stf1_id $(DOCS)
	cp stf1_sumlev $(DOCS)

$(LIBES): # in case LIB changes
