
INTERFILES = \
	mkgrid_inter.o \
	ask_for.o \
	init_head.o \
	write_grid.o \
	rotate.o \
	grid_atts.o

CMDFILES = \
	mkgrid_cmd.o \
	init_head.o \
	write_grid.o \
	rotate.o \
	grid_atts.o

PROG_CMD = $(BIN_CMD)/v.mkgrid 
PROG_INT = $(BIN_INTER)/v.mkgrid 

all:  $(PROG_CMD) $(PROG_INT)


$(PROG_CMD):  $(CMDFILES) $(DEPGISLIB) $(DEPVECTLIB) $(DEPDIG_ATTLIB)
	$(CC) $(LDFLAGS) -o $@ $(CMDFILES) $(GISLIB) $(VECTLIB) $(MATHLIB) $(XDRLIB) $(DIG_ATTLIB)

$(PROG_INT):  $(INTERFILES) $(DEPGISLIB) $(DEPVECTLIB)
	$(CC) $(LDFLAGS) -o $@ $(INTERFILES) $(GISLIB) $(VECTLIB) $(MATHLIB) $(XDRLIB) $(DIG_ATTLIB)

$(DEPGISLIB) $(DEPVECTLIB): #
