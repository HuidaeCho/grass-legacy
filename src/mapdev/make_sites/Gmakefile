EXTRA_CFLAGS	= -I../lib



all:  $(BIN)/make.sites $(BIN)/Vmake.sites
	

$(BIN)/make.sites: make_sites.o
	$(CC) $(LDFLAGS) -o $@ make_sites.o $(GISLIB) $(DIGLIB) $(MATHLIB)

$(BIN)/Vmake.sites: Vmake_sites.o
	$(CC) $(LDFLAGS) -o $@ Vmake_sites.o $(GISLIB) $(DIGLIB) $(MATHLIB)

saber_src:
