PGM=v.mkquads

OFILES = \
	mkquads_cmd.o \
	ask_for.o \
	calc_quads.o \
	convert.o \
	init_head.o \
	init_quad.o \
	reg_quads.o \
	report_quads.o \
	sites_quads.o \
	wind_quads.o \
	write_quads.o

LIBES=$(GISLIB) $(VECTLIB) $(GPROJLIB) $(COORCNVLIB) $(EXTRALIB) $(DATETIMELIB)
DEPLIBS=$(DEPGISLIB) $(DEPVECTLIB) $(DEPGPROJLIB) $(DEPCOORCNVLIB) $(EXTRALIB) $(DEPDATETIMELIB)

$(BIN_CMD)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)
