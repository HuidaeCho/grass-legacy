CC = gcc
LIBTRANS	= $(SRC)/libes/vect32/libes/libtrans.a

EXTRA_CFLAGS = $(VECT_INCLUDE)

PGM = v.build.polylines
LIST =  main.o \
	line_coords.o \
	local_dig.o \
	walk.o


$(BIN_CONTRIB_CMD)/$(PGM): $(LIST)  $(VECTLIB) $(GPROJLIB) $(LIBTRANS) $(RASTERLIB) $(DIG_ATTLIB) $(GISLIB)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(VECTLIB) $(VECTLIB) $(GPROJLIB)  $(LIBTRANS) $(RASTERLIB) $(DIG_ATTLIB)  $(GISLIB) $(MATHLIB)


$(GISLIB) $(VECTLIB): #

main.o:	global_vars.h
main.o:	global_includes.h
main.o:	line_coords.h
line_coords.o:	line_coords.h
local_dig.o:	local_dig.h
walk.o:	walk.h
walk.o:	line_coords.h