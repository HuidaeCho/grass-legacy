PGM = v.in.mif

LIST = 	lex.yy.o \
	main.o \
	attval.o \
	parse.o \
	write_lines.o \
	fields.o \
	cpoly.o \
	gbtree.o \
	store.o

LIBES=$(VECTLIB) $(GISLIB) $(DIG_ATTLIB) $(LINKMLIB)
DEPLIBS=$(DEPVECTLIB) $(DEPGISLIB) $(DEPDIG_ATTLIB) $(DEPLINKMLIB)

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

lex.yy.c : mif.l
	$(LEX) -t mif.l > $@

$(DEPVECTLIB): #
$(DEPGISLIB): # 
$(DEPDIG_ATTLIB): #
$(DEPLINKMLIB): #
