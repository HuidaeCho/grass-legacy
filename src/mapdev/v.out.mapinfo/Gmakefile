PGM=v.out.mapinfo

OFILES = \
	main.o \
	site_to_mpinfo.o \
        pnt_to_mpinfo.o \
	line_to_mpinfo.o \
	area_to_mpinfo.o \
        prune_points.o \
	wr_mpinfo_head.o \
	wr_mpinfo_coor.o \
        get_isle_xy.o

LIBES=$(VECTLIB) $(GISLIB) $(DATETIMELIB)
DEPLIBS=$(DEPVECTLIB) $(DEPGISLIB) $(DEPDATETIMELIB)
$(BIN_INTER)/$(PGM): $(OFILES) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(OFILES) $(LIBES) $(MATHLIB) $(XDRLIB)

