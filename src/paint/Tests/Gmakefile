# @(#)Gmakefile	2.1  7/14/87

INTERFACE = $(SRC)/paint/Interface/applib/lib.a
HOME=$(ETC)/paint
all:	$(HOME)/vres.test \
	$(HOME)/hres.test \
	$(HOME)/pixel.test \
	$(HOME)/char.test

####################################################################


$(HOME)/pixel.test:	ptest.o $(INTERFACE)
	$(CC) $(LDFLAGS) ptest.o $(INTERFACE) -o $@

$(HOME)/hres.test:	htest.o $(INTERFACE)
	$(CC) $(LDFLAGS) htest.o $(INTERFACE) -o $@

$(HOME)/vres.test:	vtest.o $(INTERFACE)
	$(CC) $(LDFLAGS) vtest.o $(INTERFACE) -o $@

$(HOME)/char.test:	ctest.o $(INTERFACE)
	$(CC) $(LDFLAGS) ctest.o $(INTERFACE) -o $@

######################################################################
$(INTERFACE):	# in case the interface changes
