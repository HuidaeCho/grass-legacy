	$(CC) $(LDFLAGS) -o $@ $(LIST) $(INTERFACE) $(GISLIB) $(VASKLIB) $(CURSES) $(MATHLIB) $(XDRLIB)

$(INTERFACE):	# in case the interface changes
$(GISLIB):	# in case the library changes
$(VASKLIB):	# in case the library changes
