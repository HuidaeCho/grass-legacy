PGM=g.access

LIST = main.o ask_perms.o exp_perms.o get_perms.o set_perms.o verify_perms.o

LIBES=$(GISLIB) $(VASKLIB)
DEPLIBS=$(DEPGISLIB) $(DEPVASKLIB)
$(BIN_INTER)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(LIST) $(LIBES) $(CURSES) $(MATHLIB) $(XDRLIB)

get_perms.o: access.h
set_perms.o: access.h

$(DEPGISLIB): #
$(DEPVASKLIB): #
